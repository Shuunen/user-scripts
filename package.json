{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/user-scripts/issues",
  "description": "personal heavily opinionated script I make to enhance my daily web experience",
  "devDependencies": {
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "5",
    "@typescript-eslint/parser": "5",
    "eslint": "8.20",
    "eslint-plugin-filenames": "1",
    "eslint-plugin-unicorn": "43",
    "repo-check": "1.25",
    "shuutils": "6.1",
    "tsm": "2",
    "typescript": "4",
    "uvu": "0",
    "watchlist": "0"
  },
  "files": [
    "src",
    "*"
  ],
  "homepage": "https://github.com/Shuunen/user-scripts",
  "keywords": [
    "user-script"
  ],
  "license": "GPL-3.0",
  "name": "@shuunen/user-scripts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Shuunen/user-scripts.git"
  },
  "scripts": {
    "check": "repo-check",
    "ci": "npm run lint && npm run check",
    "lint": "tsc --noEmit && eslint --fix --ignore-path .gitignore --ext .js .",
    "postversion": "git push && git push --tags",
    "preversion": "npm run ci",
    "test": "uvu -r tsm tests",
    "test:watch": "watchlist src tests -eager -- npm test"
  },
  "version": "1.0.0"
}
