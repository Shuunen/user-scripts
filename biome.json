{
  "$schema": "https://biomejs.dev/schemas/1.8.2/schema.json",
  "json": {
    "formatter": {
      "enabled": false
    }
  },
  "linter": {
    "enabled": true,
    "rules": {
      "suspicious": {
        "noRedundantUseStrict": "off"
      },
      "complexity": {
        "useSimpleNumberKeys": "off"
      },
      "recommended": true
    }
  },
  "organizeImports": {
    "enabled": true
  }
}
